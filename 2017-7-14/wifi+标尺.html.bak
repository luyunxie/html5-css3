<!doctype html>
<html>
    <head>
	      <meta http-equiv="Content-Type" content="text/html;charset=uft-8"/>

		  <title>wifi+标尺</title>
		  <meta name="keywords" content=""/>
		  <meta name="description" content=""/>

		  <style type="text/css">
		       .wifi{padding:2px;float:left;border:2px solid;border-color:transparent transparent transparent  green ;border-radius:50%}
			   
			   ul:after{content:'';display:block;clear:both;}
			   ul li{float:left;height:15px;border-right:2px solid red;padding-right:7px;list-style:none}
			   ul li:nth-child(5n){height:20px;}
			   ul li:nth-child(10n){height:25px;}
			   ul li:first-child{height:25px;}
		  </style>
	</head>
	<body>
	      <div class="wifi">
		      <div class="wifi">
			      <div class="wifi"></div>
			  </div>
		  </div>

		  <ul></ul>

		  <script>
		        
				      //alert(document.getElementsByTagName("ul")[0].nodeName);
				      for(var i= 0 ;i<100;i++){
					     var li = document.createElement("li");
				         var ul = document.getElementsByTagName("ul")[0];
						 ul.appendChild(li);
					  }

		        
		  </script>
	</body>
</html>